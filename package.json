    "cross-env": "^7.0.3",
    "laravel-mix": "5.0",
    "nodemon": "^2.0.15",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.49.7",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.14"
